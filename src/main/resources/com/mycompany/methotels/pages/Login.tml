<html t:type="layout" title="Log In to MetHotels"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
      xmlns:p="tapestry:parameter">
    <div class="row">
        <div class="span4 offset3">
                <h2>Log in.</h2>
            <t:beaneditform t:id="form" object="userLogin" submitlabel="Login" exclude="id,rola">
                <t:errors/>
                <p:sifra>
                    <t:label for="sifra"/>
                    <t:passwordfield t:id="sifra" value="userLogin.sifra"/>
                </p:sifra>
            </t:beaneditform>
        </div>
    </div>
</html>

